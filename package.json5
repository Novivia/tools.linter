{
  "name": "@auex/eslint-myrules",
  "role": "linter",
  "version": "0.6.3",
  "publishConfig": {
    "registry": "https://npm.novivia.com"
  },
  "description": "ESlint rules for AuctionEx",
  "author": "Michael Ferris",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "git+ssh://git.novivia.com/auex/tools.rules"
  },
  "bin": {
    "eslint-myrules": "./cliWrapper"
  },
  "main": "index.js",
  "scripts": {
    "build": "build-module build -p 'modifiers/**' -p 'rules/**' -p cliWrapper",
    "lint": "node cliWrapper lint .",
    "pub": "build-module publish",
    "release": "build-module release",
  },
  "eslint-myrules": {
    "modifiers": {
      "global": {
        "react": false,
        "es2015": false,
      }
    }
  },
  "devDependencies": {
    // AuctionEx dependencies.
    "@auex/build-module": "^0.2.6",
  },
  "dependencies": {
    // npm dependencies.
    "babel-eslint": "^6.1.0",
    "eslint": "^3.0.1",
    "eslint-myrules-manager": "^0.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
  }
}
