// DO NOT EDIT THIS FILE - USE eslint-myrules config
{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "node": true
  },
  "globals": {},
  "plugins": ["shadow-exception", "babel"],
  "rules": {
    "shadow-exception/no-shadow": [2, {
      "exceptions": {
        "err": true,
        "next": true,
        "callback": true,
        "cb": true,
        "res": true,
        "result": true,
        "response": true
      }
    }],
    "no-shadow": 0,
    "camelcase": [1, {
      "properties": "never"
    }],
    "consistent-this": [0, "self"],
    "curly": [2, "all"],
    "indent": [2, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": 2
    }],
    "no-multi-spaces": [2, {
      "exceptions": {
        "VariableDeclarator": true
      }
    }],
    "no-unneeded-ternary": 2,
    "no-useless-call": 2,
    "quotes": [2, "double"],
    "id-length": [2, {
      "min": 4,
      "max": 40,
      "exceptions": ["cb", "e", "err"]
    }],
    "callback-return": 2,
    "guard-for-in": 2,
    "max-depth": [1, 15],
    "max-nested-callbacks": [1, 5],
    "max-params": [1, 8],
    "no-console": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-lonely-if": 2,
    "no-new": 1,
    "no-self-compare": 2,
    "no-throw-literal": 2,
    "no-unexpected-multiline": 2,
    "no-unused-vars": [2, {
      "args": "after-used"
    }],
    "no-use-before-define": [2, "nofunc"],
    "valid-jsdoc": [2, {
      "prefer": {
        "returns": "return"
      },
      "requireReturn": false
    }],
    "wrap-iife": [2, "outside"],
    "yoda": [2, "never", {
      "exceptRange": true
    }],
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-mixed-requires": 0,
    "no-underscore-dangle": 0,
    "strict": 0,
    "array-bracket-spacing": [2, "never"],
    "brace-style": [2, "1tbs", {
      "allowSingleLine": true
    }],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "dot-location": [2, "property"],
    "lines-around-comment": [2, {
      "allowBlockStart": true,
      "beforeBlockComment": true,
      "beforeLineComment": true
    }],
    "max-len": [1, 80, 2, {
      "ignoreComments": true
    }],
    "no-floating-decimal": 2,
    "no-inline-comments": 2,
    "no-inner-declarations": 2,
    "no-multiple-empty-lines": [2, {
      "max": 2
    }],
    "no-nested-ternary": 2,
    "no-spaced-func": 2,
    "babel/object-curly-spacing": [2, "never"],
    "operator-linebreak": 2,
    "padded-blocks": [2, "never"],
    "space-before-blocks": [2, "always"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "spaced-comment": [2, "always", {
      "exceptions": ["/"]
    }]
  }
}